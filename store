/*
* Explore whether C language stores a 2D array by rows or by columns without using pointer variables.
* The program should determine the memory storage order of the 2D array elements.
*/
#include <stdio.h>

int main() {
    int A[2][3] = {{1, 2, 3}, {4, 5, 6}};
    int i, j;

    for (i = 0; i < 2; i++) {      
        for (j = 0; j < 3; j++) {    
            printf("%d ", A[i][j]);  
        }
        printf("\n"); 
    }

    return 0;
}

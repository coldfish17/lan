/*
*This program takes a positive integer as input and determines if it is a prime number or not
*/
#include<stdio.h>
int main(){
    int num,i;
    printf("Please enter a positive integer:\n");
    scanf("%d",&num);
    i=2;
    while(i<num)
     {if(num%i==0)
      {printf("no");
       return 0;
      }
     else
       i++;
     }
     printf("yes");
 return 0;
}
